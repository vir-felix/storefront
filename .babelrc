{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-stage-2",
      {
        "decoratorsLegacy": true,
        "loose": true
      }
    ],
    [
      "next/babel",
      {
        "preset-env": {
          "targets": {
            "node": "current",
            "browsers": [
              "last 2 versions"
            ]
          }
        }

      }
    ]
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ]
      }
    ]
  ]
}
