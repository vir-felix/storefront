{
  "presets": [
    [ "next/babel", {
      "preset-env": {
        "modules": "commonjs",
        "targets": {
          "node": "current",
          "browsers": [
            "last 2 versions",
            "ie 11"
          ]
        }
      }
    } ]
  ],
  "env": {
    "development": {
      "plugins": [
        "babel-plugin-styled-components",
        [ "module-resolver", { "root": [ "./src", "./config" ] } ],
        [ "@babel/plugin-proposal-decorators", { "legacy": true } ],
        [ "@babel/plugin-proposal-class-properties", { "loose": true } ]
      ]
    },
    "production": {
      "plugins": [
        "babel-plugin-styled-components",
        [ "module-resolver", { "root": [ "./src", "./config" ] } ],
        [ "@babel/plugin-proposal-decorators", { "legacy": true } ],
        [ "@babel/plugin-proposal-class-properties", { "loose": true } ]
      ]
    },
    "jesttest": {
      "plugins": [
        "babel-plugin-styled-components",
        [ "module-resolver", { "root": [ "./src", "./config" ] } ],
        [ "@babel/plugin-proposal-decorators", { "legacy": true } ],
        [ "@babel/plugin-proposal-class-properties", { "loose": true } ]
      ]
    }
  }
}
